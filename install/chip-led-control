sudo rm -rf /usr/bin/chip-led-white-on
sudo touch /usr/bin/chip-led-white-on
sudo echo sudo i2cset -f -y 0 0x34 0x93 0x1 >> /usr/bin/chip-led-white-on
sudo chmod 755 /usr/bin/chip-led-white-on
sudo rm -rf /usr/bin/chip-led-white-off
sudo touch /usr/bin/chip-led-white-off
sudo echo sudo i2cset -f -y 0 0x34 0x93 0x0 >> /usr/bin/chip-led-white-off
sudo chmod 755 /usr/bin/chip-led-white-off
echo install complete
